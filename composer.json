{
    "name": "samuelnitsche/laravel-auth-log",
    "description": "Laravel Auth Log provides authentication logger and notification for Laravel.",
    "keywords": [
        "laravel",
        "authentication",
        "log",
        "notification"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Nitsche",
            "email": "mail@samynitsche.de"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/auth": "~5.6",
        "illuminate/bus": "~5.6",
        "illuminate/console": "~5.6",
        "illuminate/contracts": "~5.6",
        "illuminate/database": "~5.6",
        "illuminate/http": "~5.6",
        "illuminate/notifications": "~5.6",
        "illuminate/support": "~5.6"
    },
    "require-dev": {
        "orchestra/database": "~3.5",
        "orchestra/testbench": "~3.5",
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "SamuelNitsche\\AuthLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SamuelNitsche\\AuthLog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "SamuelNitsche\\AuthLog\\AuthLogServiceProvider"
            ]
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required to use the Slack transport (~6.0)",
        "nexmo/client": "Required to use the Nexmo transport (~1.0)."
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
